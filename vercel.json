{
  "version": 2,
  "buildCommand": "cd client && npm install && vite build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.{js,ts}": {
      "runtime": "@vercel/node@20"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
